        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "express": "^4.18.3",
        "express-jwt": "^8.4.1",
        "express-validator": "^7.0.1",
        "http-errors": "^2.0.0",
        "js-yaml": "^4.1.0",
        "jsonwebtoken": "^9.0.2",
